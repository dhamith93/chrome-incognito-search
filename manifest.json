{
    "name": "Incognito Search",
    "description": "Launch search in incognito",
    "version": "1.0.0",
    "icons": {
        "128": "img/icon_128.png"
    },
    "permissions": [
        "contextMenus",
        "tabs",
        "storage"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "icon": "img/icon_19.png",
        "default_popup": "popup.html"
    },
    "manifest_version": 2
}